@{
    ViewBag.Title = "User Dashboard";
}


<link href="~/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
<link href="~/fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
<script src="~/Scripts/jquery-1.11.1.js"></script>
<script src="~/bootstrap/js/bootstrap.min.js"></script>
<link href="~/Content/Site.css" rel="stylesheet" />


<div class="container">

    <div class="logo col-sm-12 text-center col-md-12 ">
        
    </div>
    <div class="clearfix"></div>
    <fieldset>

        <legend>Panel de Administración del Usuario</legend>

        <div class="panel">

            <div class="panel-body">
               
             
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-user"></span>
                    @Html.ActionLink("Registrar Miembro ", "Create", "RegisterMember")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-list"></span>
                    @Html.ActionLink("Listado de pagos ", "PaymentDetails", "Payment")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-calendar"></span>
                    @Html.ActionLink("Renovación de Pago", "RenewalDetails", "Renewal")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-download-alt"></span>
                    @Html.ActionLink("Recibo / Candidato ", "Generatereceipt", "receipt")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-download-alt"></span>
                    @Html.ActionLink("Informe Anual ", "YearwiseReport", "YearwiseCollection")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-download-alt"></span>
                    @Html.ActionLink("Informe mensual ", "MonthwiseReport", "MonthwiseCollection")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-download-alt"></span>
                    @Html.ActionLink("Detalles del Informe de Miembro", "DetailsMemberReport", "DetailsReport")
                </div>
                <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-list"></span>
                    @Html.ActionLink("Todos los miembros ", "AllMemberList", "Payment")
                </div>
                  <div class="col-md-3  dashboard-icon col-sm-3">
                    <span class="glyphicon glyphicon-calendar"></span>
                    @Html.ActionLink("Informe de Renovación", "RenewalReport", "RenewalSearch")
                </div>
            </div>
        </div>
    </fieldset>

</div>
